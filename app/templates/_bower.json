{
  "name": "<%= config.libraryName.slugified %>",
  "version": "0.1.0",
  "authors": [{
    "name": "<%= config.author.name %>",
    "email": "<%= config.author.email %>"
  }],
  "main": ["dist/<%= config.libraryName.slugified %>.js"],
  "ignore": [
    "src",
    "test",
    "gulpfile.js",
    "karma-*.conf.js",
    "**/.*"
  ],
  "dependencies": {
    "angular": "~1.5.0"
    <% if (config.includeAngularModuleResource) { %>,
    "angular-resource": "~1.5.0"
    <% } %>
    <% if (config.includeAngularModuleCookies) { %>,
    "angular-cookies": "~1.5.0"
    <% } %>
    <% if (config.includeAngularModuleSanitize) { %>,
    "angular-sanitize": "~1.5.0"
    <% } %>
  },
  "devDependencies": {
    "angular-mocks": "~1.5.0",
    "angular-scenario": "~1.5.0"
  }
}
